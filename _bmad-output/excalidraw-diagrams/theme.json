{
  "name": "Classic Wireframe",
  "colors": {
    "background": "#ffffff",
    "container": "#f5f5f5",
    "border": "#9e9e9e",
    "text": "#424242",
    "primary": "#1976d2",
    "secondary": "#424242",
    "accent": "#2196f3",
    "success": "#4caf50",
    "warning": "#ff9800",
    "error": "#f44336"
  },
  "typography": {
    "fontFamily": "Virgil, Segoe UI",
    "fontSize": {
      "heading1": 32,
      "heading2": 24,
      "heading3": 20,
      "body": 16,
      "caption": 12
    }
  },
  "spacing": {
    "unit": 8,
    "small": 8,
    "medium": 16,
    "large": 24,
    "xlarge": 32
  },
  "wireframe": {
    "strokeWidth": 2,
    "roughness": 0,
    "strokeStyle": "solid",
    "fillStyle": "solid",
    "opacity": 100
